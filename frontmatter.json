{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "template": "[[workspace]]/.frontmatter/templates/post-template.md",
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true,
          "dateFormat": "yyyy-MM-dd"
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Title",
          "name": "Title",
          "type": "string"
        },
        {
          "title": "eleventyExcludeFromCollections",
          "name": "eleventyExcludeFromCollections",
          "type": "boolean"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "11ty",
  "frontMatter.preview.host": "http://localhost:8080",
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "blog",
      "path": "[[workspace]]/content/blog"
    },
    {
      "title": "tools",
      "path": "[[workspace]]/content/blog/tools"
    },
    {
      "title": "howtos",
      "path": "[[workspace]]/content/blog/howtos"
    },
    {
      "title": "jobgivers",
      "path": "[[workspace]]/content/blog/jobgivers"
    },
    {
      "title": "jobseekers",
      "path": "[[workspace]]/content/blog/jobseekers"
    },
    {
      "path": "[[workspace]]/public"
    },
    {
      "title": "img",
      "path": "[[workspace]]/public/img"
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.content.snippets": {}
}